{
  "name": "romeoz/rock-sphinx",
  "description": "Object Relational Mapping (ORM) for Sphinx Search.",
  "keywords": [
    "sphinx",
    "sphinxsearch",
    "search engine",
    "search",
    "active record",
    "sphinxql"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "email": "serggalka@gmail.com",
    "issues": "https://github.com/romeOz/rock-sphinx/issues",
    "source": "https://github.com/romeOz/rock-sphinx"
  },
  "authors": [
    {
      "name": "Ruslan",
      "email": "serggalka@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "romeoz/rock-db": "0.12.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7.0",
    "romeoz/rock-dataprovider": "0.10.*",
    "romeoz/rock-cache": "0.11.*",
    "romeoz/rock-validate": "0.10.*",
    "romeoz/rock-sanitize": "0.10.*"
  },
  "suggest": {
    "romeoz/rock-dataprovider": "required by Rock Data provider",
    "romeoz/rock-cache": "required by Rock Cache",
    "romeoz/rock-validate": "required by Rock Valiadte",
    "romeoz/rock-sanitize": "required by Rock Sanitize",
    "romeoz/rock-behaviors": "required by Rock Behaviors"
  },
  "autoload": {
    "psr-4": {
      "rock\\sphinx\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.12-dev"
    }
  }
}